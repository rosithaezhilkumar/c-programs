#include <stdio.h>
#include <stdio.h>

int main()
{
    int n,x,sum;
    printf("Enter the size of array");
    scanf("%d",&n);
    int array[n];
 
   for (x = 0; x < n; x++)
   {
     scanf("%d", &array[x]);
     sum = sum + array[x];
   }
 
   printf("Sum = %d\n", sum);

    return 0;
}
 
struct triangle
{
    float height;
    float base;
};
 
int main()
{
    float h,b;
    printf("enter the height of the triangle");
    scanf("%f",&h);
    printf("enter the base of the triangle");
    scanf("%f",&b);
    float a;
    a=triangle(h, b);
    printf( "area value is : %f\n", area );
 

    return 0;
}
float areaf(struct triangle t)
{
    float a=0.5*t.height*t.base;
    return a;
}
