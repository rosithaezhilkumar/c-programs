#include <stdio.h>

int main()

{
  float num1=0,den1=0,num2=0,den2=0,ans=0;

  printf("enter num1 and den1 values");
  scanf("%f%f",&num1,&den1); 

  printf("enter num2 and den2 values");
  scanf("%f%f",&num2,&den2); 
  
  ans=((num2*den1)+(num1*den2))/(den1*den2);
   printf("the ans is %f",ans);
    return 0;
}
